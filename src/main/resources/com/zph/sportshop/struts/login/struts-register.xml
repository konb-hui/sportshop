<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
        "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	<package name="register" namespace="/" extends="struts-default">
		<action name="registerAction_*" method="{1}" class="registerAction">
			<result name="homepage">main.jsp</result>
			<result name="action2action" type="redirectAction">register.jsp</result>
			<result name="register">register.jsp</result>
		</action>
	</package>
</struts>
