<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
        "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	<package name="brand" namespace="/" extends="interceptor">
		<action name="brandAction_*" method="{1}" class="brandAction">
			<result name="listBrand">WEB-INF/jsp/sportshop/basedata/admin/adminBrand.jsp</result>
