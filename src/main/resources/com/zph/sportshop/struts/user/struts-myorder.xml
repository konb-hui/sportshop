<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
        "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	<package name="myorder" namespace="/" extends="interceptor">
		<action name="myorderAction_*" method="{1}" class="myorderAction">
			<result name="confirmorder">orderConfirm.jsp</result>
			<result name="login">login.jsp</result>
			<result name="listorder">WEB-INF/jsp/sportshop/basedata/user/list.jsp</result>
			<result name="listOrderForAdmin">WEB-INF/jsp/sportshop/basedata/admin/adminAllOrder.jsp</result>
			<result name="listReceivedOrder">WEB-INF/jsp/sportshop/basedata/admin/adminReceivedOrder.jsp</result>
			<result name="listCompletedOrder">WEB-INF/jsp/sportshop/basedata/admin/adminCompletedOrder.jsp</result>
		</action>
	</package>
</struts>
